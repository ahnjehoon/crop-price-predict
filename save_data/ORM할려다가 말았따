from sqlalchemy.ext.declarative import declarative_base

Base = declarative_base()

class FilePaths(Base):
    __tablename__ = 'product_predict'
    __table_args__ = {'mysql_engine':'InnoDB'}
	#__table_args__ = {'mysql_collate': 'utf8_general_ci'}


	id = Column(Integer, primary_key=True)
	aucsecode = Column(String(100))
	aucsenm = Column(String(100))
	catgorycode = Column(String(100))
	catgorynewcode = Column(String(100))
	catgorynewnm = Column(String(100))
	catgorynm = Column(String(100))
	cprinsttcode = Column(String(100))
	cprinsttnewcode = Column(String(100))
	cprmtccode = Column(String(100))
	cpruseprdlstcode = Column(String(100))
	cpruseprdlstnm = Column(String(100))
	delngde = Column(String(100))
	delngprut = Column(String(100))
	delngqy = Column(String(100))
	insttnewnm = Column(String(100))
	insttnm = Column(String(100))
	ledgno = Column(String(100))
	rnum = Column(String(100))
	sbidpric = Column(String(100))
	shipmntsecode = Column(String(100))
	shipmntsenm = Column(String(100))
	sleseqn = Column(String(100))
	stdfrmlcnewcode = Column(String(100))
	stdfrmlcnewnm = Column(String(100))
	stdmgnewcode = Column(String(100))
	stdmgnewnm = Column(String(100))
	stdmtcnewcode = Column(String(100))
	stdmtcnewnm = Column(String(100))
	stdprdlstcode = Column(String(100))
	stdprdlstnewcode = Column(String(100))
	stdprdlstnewnm = Column(String(100))
	stdprdlstnm = Column(String(100))
	stdqlitynewcode = Column(String(100))
	stdqlitynewnm = Column(String(100))
	stdspciescode = Column(String(100))
	stdspciesnewcode = Column(String(100))
	stdspciesnewnm = Column(String(100))
	stdspciesnm = Column(String(100))
	stdunitnewcode = Column(String(100))
	stdunitnewnm = Column(String(100))
	whsalmrktcode = Column(String(100))
	whsalmrktnewcode = Column(String(100))
	whsalmrktnewnm = Column(String(100))
	whsalmrktnm = Column(String(100))

Base.metadata.create_all(engine)